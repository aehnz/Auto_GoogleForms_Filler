{
  "manifest_version": 3,
  "name": "Google Form AutoFiller",
  "version": "1.0",
  "description": "Auto-fills Google Forms instantly with one click.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"]
    }
  ]
}
